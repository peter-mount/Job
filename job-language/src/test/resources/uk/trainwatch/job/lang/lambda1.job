/*
 * lambda1  Basic lambda functions
 */
{
    // define a simple function
    add = (a,b)->a+b;

    sub = (a,b) -> a - b;

    // Now some simple sums
    log add(1,2);
    log add(7,3);
    log sub(10,4);
    log sub(add(3,7),6);

    // Simple lambda with one arg so no (), simply wrap a string with ":"
    logger = s -> ":" + s + ":";
    log logger("hello");
    
}
